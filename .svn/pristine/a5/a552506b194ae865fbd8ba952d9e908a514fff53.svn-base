package com.bogaware.service.accounts.data;
public final class BankTransactionPaymentMeta {
	private String byOrderOf;
	private String payee;
	private String payer;
	private String paymentMethod;
	private String paymentProcessor;
	private String ppdId;
	private String reason;
	private String referenceNumber;
	
	public BankTransactionPaymentMeta(String orderOfMeta, String payeeMeta, String payerMeta, String paymentMethodMeta, String paymentProcMeta, String ppdIdMeta, String reasonMeta, String referenceNumMeta){
		byOrderOf = orderOfMeta;
		payee = payeeMeta;
		payer = payerMeta;
		paymentMethod = paymentMethodMeta;
		paymentProcessor = paymentProcMeta;
		ppdId = ppdIdMeta;
		reason = reasonMeta;
		referenceNumber = referenceNumMeta;
	}
	
	public String getByOrderOf() {
	  return byOrderOf;
	}
	
	public String getPayee() {
	  return payee;
	}
	
	public String getPayer() {
	  return payer;
	}
	
	public String getPaymentMethod() {
	  return paymentMethod;
	}
	
	public String getPaymentProcessor() {
	  return paymentProcessor;
	}
	
	public String getPpdId() {
	  return ppdId;
	}
	
	public String getReason() {
	  return reason;
	}
	
	public String getReferenceNumber() {
	  return referenceNumber;
	}
}